
<!DOCTYPE HTML>
<html>
<head>
	<title>Footwear - Free Bootstrap 4 Template by Colorlib</title>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

	<link href="https://fonts.googleapis.com/css?family=Montserrat:300,400,500,600,700" rel="stylesheet">
	<link href="https://fonts.googleapis.com/css?family=Rokkitt:100,300,400,700" rel="stylesheet">

	<!-- Animate.css -->
	<link rel="stylesheet" href="../anhweb/css/animate.css">
	<!-- Icomoon Icon Fonts-->
	<link rel="stylesheet" href="../anhweb/css/icomoon.css">
	<!-- Ion Icon Fonts-->
	<link rel="stylesheet" href="../anhweb/css/ionicons.min.css">
	<!-- Bootstrap  -->
	<link rel="stylesheet" href="../anhweb/css/bootstrap.min.css">

	<!-- Magnific Popup -->
	<link rel="stylesheet" href="../anhweb/css/magnific-popup.css">

	<!-- Flexslider  -->
	<link rel="stylesheet" href="../anhweb/css/flexslider.css">

	<!-- Owl Carousel -->
	<link rel="stylesheet" href="../anhweb/css/owl.carousel.min.css">
	<link rel="stylesheet" href="../anhweb/css/owl.theme.default.min.css">

	<!-- Date Picker -->
	<link rel="stylesheet" href="../anhweb/css/bootstrap-datepicker.css">
	<!-- Flaticons  -->
	<link rel="stylesheet" href="../anhweb/fonts/flaticon/font/flaticon.css">

	<!-- Theme style  -->
	<link rel="stylesheet" href="../anhweb/css/style.css">

</head>
<body>

	<div class="colorlib-loader"></div>

	<div id="page">
		@await Html.PartialAsync("_HeaderPartial")

		<div class="colorlib-product">
			<div class="container">
				<div class="row">
					<div class="col-sm-8 offset-sm-2 text-center colorlib-heading">
						<h2>Best Sellers</h2>
					</div>
				</div>
				<div id="displayCauthus" class="row row-pb-md">
					@RenderBody()
				</div>
				<div class="row">
					<div class="col-md-12 text-center">
						<p><a href="#" class="btn btn-primary btn-lg">Shop All Products</a></p>
					</div>
				</div>
			</div>
		</div>

		@await Html.PartialAsync("_FooterPartial")
	</div>

	<div class="gototop js-top">
		<a href="#" class="js-gotop"><i class="ion-ios-arrow-up"></i></a>
	</div>

	<!-- jQuery -->
	<script src="../anhweb/js/jquery.min.js"></script>
	<!-- popper -->
	<script src="../anhweb/js/popper.min.js"></script>
	<!-- bootstrap 4.1 -->
	<script src="../anhweb/js/bootstrap.min.js"></script>
	<!-- jQuery easing -->
	<script src="../anhweb/js/jquery.easing.1.3.js"></script>
	<!-- Waypoints -->
	<script src="../anhweb/js/jquery.waypoints.min.js"></script>
	<!-- Flexslider -->
	<script src="../anhweb/js/jquery.flexslider-min.js"></script>
	<!-- Owl carousel -->
	<script src="../anhweb/js/owl.carousel.min.js"></script>
	<!-- Magnific Popup -->
	<script src="../anhweb/js/jquery.magnific-popup.min.js"></script>
	<script src="../anhweb/js/magnific-popup-options.js"></script>
	<!-- Date Picker -->
	<script src="../anhweb/js/bootstrap-datepicker.js"></script>
	<!-- Stellar Parallax -->
	<script src="../anhweb/js/jquery.stellar.min.js"></script>
	<!-- Main -->
	<script src="../anhweb/js/main.js"></script>

</body>
</html>

